{
  "name": "timesheet-helper",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.9.0",
  "description": "A helper for timesheet",
  "author": "Phil Liu",
  "license": "MIT",
  "keywords": [],
  "sideEffects": false,
  "scripts": {
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "start": "esno pnpm run start --filter=extract-log"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.3.2",
    "@antfu/ni": "^0.23.0",
    "@antfu/utils": "^0.7.10",
    "@types/node": "^22.5.4",
    "bumpp": "^9.5.2",
    "eslint": "^9.9.1",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.10",
    "pnpm": "^9.9.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0",
    "vite": "^5.4.3",
    "vitest": "^2.0.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
